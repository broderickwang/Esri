<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="marc.com.esri.MainActivity">

    <com.esri.android.map.MapView
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapoptions.MapType="Streets"
        mapoptions.center="34.056215, -117.195668"
        mapoptions.ZoomLevel="16"></com.esri.android.map.MapView>

    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/club_home_fa_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        app:fab_labelStyle="@android:style/Widget.SeekBar">
        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/detailed_action_update_notice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@android:color/holo_green_light"
            app:fab_icon="@android:drawable/ic_media_next"
            app:fab_colorPressed="@android:color/background_light"
            app:fab_title="NEXT"/>

        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fab_colorNormal="@android:color/holo_green_light"
            app:fab_icon="@android:drawable/ic_menu_share"
            app:fab_colorPressed="@android:color/background_light"
            app:fab_title="LOCATION"/>
    </com.getbase.floatingactionbutton.FloatingActionsMenu>
</RelativeLayout>
